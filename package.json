    "format": "prettier --write \"./**/*.{ts,js,json,md}\"",
    "lint-fix": "eslint \"./**/*.{ts,js}\" --fix --no-error-on-unmatched-pattern",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "ts-mocha **/*.spec.ts"
